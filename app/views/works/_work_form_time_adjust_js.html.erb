<script type="text/javascript">
    $(document).ready(function () {
        //get first not filled time interval
        from_id =parseInt($('td.timeline_minutes[style*="green;"]:last').attr("id")) + 10;
        console.log(from_id );

        if( !from_id) {
            from_id = 0;
        }

        // set from hour time
        $("#work_from_4i [value='"+getHourFromId(from_id)+"']").attr("selected", "selected");
        // set from minute time
        $("#work_from_5i [value='"+getMinutesFromId(from_id)+"']").attr("selected", "selected");

        // set to selects
        to_id = from_id + 60;
        $("#work_to_4i [value='"+getHourFromId(to_id)+"']").attr("selected", "selected");
        $("#work_to_5i [value='"+getMinutesFromId(to_id)+"']").attr("selected", "selected");
    });

    function getHourFromId( id ) {
        if( id < 60 ) {
            return '00';
        }else{
            hour = Math.floor(id/60)
            return hour<10 ? '0'+hour : hour;
        }
    }

    function getMinutesFromId( id ) {
        if( id == 0) {
            return '00';
        } else if (id < 60) {
            return id;
        } else {
            return id%60 == 0 ? '00' : id%60;
        }
    }

</script>